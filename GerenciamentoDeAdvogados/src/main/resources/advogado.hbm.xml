<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="br.com.unipe.gerenciamentoAdvogados.model.vo.Advogado"
		table="advogado">
		<id name="id" type="long">
			<generator class="native"></generator>
		</id>
		<property name="createdOn" type="timestamp"></property>
		<property name="numeroOab" type="string"></property>		

		<many-to-one name="usuario" column="usuario_id" unique="true"
			class="br.com.unipe.gerenciamentoAdvogados.model.vo.Usuario">
		</many-to-one>
	</class>
</hibernate-mapping>