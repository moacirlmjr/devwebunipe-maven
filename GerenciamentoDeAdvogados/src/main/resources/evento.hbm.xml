<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd" >
<hibernate-mapping>
	<class name="br.com.unipe.gerenciamentoAdvogados.model.vo.Evento"
		table="advogado">
		<id name="id" type="long">
			<generator class="native"></generator>
		</id>
		<property name="createdOn" type="timestamp"></property>
		<property name="nome" type="string"></property>
		<property name="data" type="timestamp"></property>

		<set name="advogados" table="advogado_evento">
			<key>
				<column name="evento_id" not-null="true"></column>
			</key>
			<many-to-many class="br.com.unipe.gerenciamentoAdvogados.model.vo.Advogado">
				<column name="advogado_id" not-null="true"></column>
			</many-to-many>
		</set>
	</class>
</hibernate-mapping>